    "@near-js/transactions": "^0.2.1",
    "@near-wallet-selector/core": "^8.5.2",
    "@near-wallet-selector/wallet-utils": "^8.5.2",
    "@oasisprotocol/sapphire-hardhat": "^2.19.4",
    "axios": "^1.6.7",
    "bitcoinjs-lib": "^6.1.5",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0",
    "bs58check": "^3.0.1",
    "buffer": "^6.0.3",
    "crypto": "^1.0.1",
