    "ecpair": "^2.1.0",
    "elliptic": "^6.5.4",
    "ethers": "^6.11.1",
    "keccak": "^3.0.4",
    "near-api-js": "^4.0.0",
    "next": "14.1.0",
    "openai": "^4.33.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.50.1",
    "react-icons": "^5.0.1",
    "react-toastify": "^10.0.4",
    "viem": "^2.9.16"
